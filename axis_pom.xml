<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XML.Schema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelversion>4.0.0</modelversion>
	<groupId>gen</groupId>
	<artifactId>Connect24FinancialAPI</artifactId>
	<version>2.7.0</version>
	<pacakaging>iib-ci</pacakaging>
	<build>
	<defaultGoal>package</defaultGoal>
	<resource>
	<resource>
	<directory>.</directory>
	</resource>
	</resource>
	<plugins>
	<plugin>
	<groupId>com.axisb.maven.plugins</groupId>
	<artifactId>iib-maven-plugin</artifactId>
	<version>1.0</version>
	<extensions>true</extensions>
	<configuration></configuration>
	</plugin>
	<plugin>
	<groupId>org.apache.maven.plugins</groupId>
	<artifactId>maven-deploy-plugin</artifactId>
	<executions>
	<execution>
	<id>custom-deploy</id>
	<phase>deploy</phase>
	</execution>
	</executions>
	</plugin>
	</plugins>
	</build>
	<distributionManagement>
	<respository>
	<id>esblocal</id>
	<name>Corporate Repository</name>
	<url>https://artifactory.axisb.com/artifactory/esb-maven-local/</url>
	<layout>default</layout>
	</respository>
	</distributionManagement>
	</project>
